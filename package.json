{
  "name": "RoboRumble",
  "version": "0.0.0",
  "description": "An online turn-based strategy game. Players build robots and program them to win contests.",
  "main": "server.js",
  "scripts": {
    "test": "grunt"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/SamGraber/Robo-Rumble.git"
  },
  "keywords": [
    "robo",
    "rumble",
    "rally",
    "mean",
    "angular",
    "express",
    "mongo",
    "node"
  ],
  "engines": {
    "node": "0.10.x",
    "npm": "1.4.x"
  },
  "author": "Sam Graber",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/SamGraber/Robo-Rumble/issues"
  },
  "homepage": "https://github.com/SamGraber/Robo-Rumble",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc && echo Build complete",
    "build.watch": "tsc -w",
    "test": "karma start config/karma.conf.js",
    "test.debug": "karma start config/karma.debug.conf.js",
    "build-test": "npm run build && npm run test",
    "build-test.watch": "npm run build && npm-run-all --parallel build.watch test.debug"
  },
  "dependencies": {
    "systemjs": "^0.19.41"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/sinon": "1.16.31",
    "chai": "^3.5.0",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "npm-run-all": "^4.0.0",
    "sinon": "^1.17.7",
    "typescript": "^2.1.4"
  }
}
